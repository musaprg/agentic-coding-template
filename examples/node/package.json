{
  "name": "agentic-coding-example",
  "version": "1.0.0",
  "description": "Example Node.js project for agentic coding template",
  "main": "hello.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "node hello.js",
    "dev": "nodemon hello.js"
  },
  "keywords": [
    "agentic",
    "coding",
    "template",
    "example"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.0",
    "@eslint/js": "^8.54.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!*.test.js",
      "!jest.config.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
